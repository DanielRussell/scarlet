{
	"name": "robyn",
	"version": "2.1.2",
	"author": "RED Interactive <geeks@ff0000.com>",
	"description": "A highly customizable boilerplate to initialize common projects.",
	"repository": {
		"type": "git",
		"url": "https://github.com/ff0000/red-boilerplate.git",
		"branch": "master"
	},
	"systemDependencies": {
		"git": ">=1.8.2",
		"grunt": "<0.4.x",
		"node": ">=0.10.x",
		"ruby": {
			"version": ">= 1.8.7",
			"install": "https://gist.github.com/3900651",
			"upgrade": "brew update && brew upgrade rbenv"
		},
		"gem": ">= 1.3.6",
		"bundle": {
			"version": ">= 1.2.0",
			"install": "gem install bundler",
			"upgrade": "gem update bundler"
		}
	},
	"engines": {
		"node": ">=0.9"
	},
	"installedPlugins": {
		"caboose": {
			"version": "2.4.0",
			"description": "RED Interactive's Sass + Compass Framework."
		},
		"rosy": {
			"description": "An inheritable / extendable JavaScript framework."
		}
	},
	"config": {
		"vars": {
			"PROJECT_NAME": "scarlet",
			"PROJECT_TITLE": "Scarlet"
		},
		"dirs": {
			"robyn": ".robyn",
			"tasks": "robyn/tasks",
			"config": "robyn/config"
		},
		"requiredPaths": [
			".robyn",
			"node_modules",
			".bundle",
			"Gemfile",
			"Gemfile.lock"
		],
		"excludedPaths": [
			".git",
			".robyn",
			"node_modules",
			".bundle",
			".sass-cache"
		]
	},
	"scripts": {
		"install": [
			".robyn/plugins/caboose/config/init/caboose.js"
		],
		"update": [
			".robyn/plugins/caboose/config/init/caboose.js"
		]
	},
	"initialized": true
}
